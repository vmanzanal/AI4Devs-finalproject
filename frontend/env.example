# SEPE Templates Comparator Frontend - Environment Variables Example
# Copy this file to .env.local and update the values for your environment

# API Configuration
VITE_API_BASE_URL=http://localhost:8000/api/v1

# Application Configuration
VITE_APP_TITLE=SEPE Templates Comparator
VITE_APP_DESCRIPTION=Compare and manage SEPE PDF templates

# Development Configuration
VITE_DEV_MODE=true
VITE_SHOW_DEBUG_INFO=false

# File Upload Configuration
VITE_MAX_FILE_SIZE_MB=50
VITE_ALLOWED_FILE_TYPES=.pdf

# Feature Flags
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_ANALYTICS=false
